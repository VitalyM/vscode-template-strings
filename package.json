{
    "name": "template-strings",
    "displayName": "Template Strings",
    "description": "Convert string to template string with argument in place",
    "version": "0.0.1",
    "publisher": "vit-m",
    "engines": {
        "vscode": "^1.20.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "activationEvents": [
        "onCommand:extension.insertArg"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.insertArg",
                "title": "Insert Argument",
                "category": "Template String"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.0.0",
        "@types/util.promisify": "^1.0.0",
        "tslint": "^5.8.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.6"
    },
    "dependencies": {
        "util.promisify": "^1.0.0"
    }
}
